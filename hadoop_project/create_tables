CREATE TABLE Source (
  sourceId BIGINT ,
  scienceCcdExposureId BIGINT ,
  filterId BIGINT ,
  objectId BIGINT ,
  movingObjectId BIGINT ,
  procHistoryId BIGINT ,
  ra FLOAT  ,
  raSigmaForDetection float ,
  raSigmaForWcs float ,
  decl FLOAT  ,
  declSigmaForDetection float ,
  declSigmaForWcs float ,
  htmId20 BIGINT ,
  xFlux FLOAT  ,
  xFluxSigma float ,
  yFlux FLOAT  ,
  yFluxSigma float ,
  raFlux FLOAT  ,
  raFluxSigma float ,
  declFlux FLOAT  ,
  declFluxSigma float ,
  xPeak FLOAT  ,
  yPeak FLOAT  ,
  raPeak FLOAT  ,
  declPeak FLOAT  ,
  xAstrom FLOAT  ,
  xAstromSigma float ,
  yAstrom FLOAT  ,
  yAstromSigma float ,
  raAstrom FLOAT  ,
  raAstromSigma float ,
  declAstrom FLOAT  ,
  declAstromSigma float ,
  raObject FLOAT  ,
  declObject FLOAT  ,
  taiMidPoint FLOAT  ,
  taiRange float ,
  psfFlux FLOAT  ,
  psfFluxSigma float ,
  apFlux FLOAT  ,
  apFluxSigma float ,
  modelFlux FLOAT  ,
  modelFluxSigma float ,
  petroFlux FLOAT  ,
  petroFluxSigma float ,
  instFlux FLOAT  ,
  instFluxSigma float ,
  nonGrayCorrFlux FLOAT  ,
  nonGrayCorrFluxSigma float ,
  atmCorrFlux FLOAT  ,
  atmCorrFluxSigma float ,
  apDia float ,
  Ixx float ,
  IxxSigma float ,
  Iyy float ,
  IyySigma float ,
  Ixy float ,
  IxySigma float ,
  psfIxx float ,
  psfIxxSigma float ,
  psfIyy float ,
  psfIyySigma float ,
  psfIxy float ,
  psfIxySigma float ,
  e1_SG float ,
  e1_SG_Sigma float ,
  e2_SG float ,
  e2_SG_Sigma float ,
  resolution_SG float ,
  shear1_SG float ,
  shear1_SG_Sigma float ,
  shear2_SG float ,
  shear2_SG_Sigma float ,
  sourceWidth_SG float ,
  sourceWidth_SG_Sigma float ,
  shapeFlag_SG BIGINT ,
  snr float ,
  chi2 float ,
  sky float ,
  skySigma float ,
  extendedness BIGINT ,
  flux_Gaussian FLOAT  ,
  flux_Gaussian_Sigma float ,
  flux_ESG FLOAT  ,
  flux_ESG_Sigma float ,
  sersicN_SG float ,
  sersicN_SG_Sigma float ,
  radius_SG float ,
  radius_SG_Sigma float ,
  flux_flux_SG_Cov float ,
  flux_e1_SG_Cov float ,
  flux_e2_SG_Cov float ,
  flux_radius_SG_Cov float ,
  flux_sersicN_SG_Cov float ,
  e1_e1_SG_Cov float ,
  e1_e2_SG_Cov float ,
  e1_radius_SG_Cov float ,
  e1_sersicN_SG_Cov float ,
  e2_e2_SG_Cov float ,
  e2_radius_SG_Cov float ,
  e2_sersicN_SG_Cov float ,
  radius_radius_SG_Cov float ,
  radius_sersicN_SG_Cov float ,
  sersicN_sersicN_SG_Cov float ,
  flagForAssociation BIGINT ,
  flagForDetection BIGINT ,
  flagForWcs BIGINT
)
