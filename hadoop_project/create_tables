drop table if exists RefSrcMatch;
drop table if exists Science_Ccd_Exposure_Metadata;
drop table if exists Object;
drop table if exists Source;
CREATE TABLE RefSrcMatch (
  refObjectId BIGINT ,
  sourceId BIGINT ,
  refRa FLOAT  ,
  refDec FLOAT  ,
  angSep FLOAT  ,
  nRefMatches BIGINT ,
  nSrcMatches BIGINT ,
  closestToRef BIGINT ,
  closestToSrc BIGINT ,
  flags BIGINT
)
row format delimited fields terminated by ',';

CREATE TABLE Science_Ccd_Exposure_Metadata (
  scienceCcdExposureId BIGINT,
  metadataKey varchar ,
  exposureType BIGINT ,
  BIGINTValue BIGINT ,
  doubleValue FLOAT ,
  stringValue varchar
)
row format delimited fields terminated by ',';

